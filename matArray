#include<stdio.h>
void main()
{
int r,c,i,j,a[10][10];
printf("enter number of rows\n");
scanf("%d",&r);
printf("enter number of columns\n");
scanf("%d",&c);
for(i=1;i<=r;i++)
{
for(j=1;j<=c;j++)
{
printf("enter elements\n");
scanf("%d",&a[i][j]);
}
printf("\n");
}
for(i=1;i<=r;i++)
{
for(j=1;j<=c;j++)
{
printf("%d",a[i][j]);
}
printf("\n");
}
}
